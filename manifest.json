{
    "name": "Nico-video gazer",
    "version": "0.1",
    "manifest_version": 2,
    "description": "ニコニコ動画を動画だけにします。",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "page_action": {
        "default_icon": "icon19.png",
        "default_title": "Yea!"
    }
}
